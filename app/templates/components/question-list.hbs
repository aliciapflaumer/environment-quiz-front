<h3 class="quiz-header">{{quiz.title}}</h3>

<h1>question list: this is the place to edit quiz</h1>

{{create-question-form createQuestion='createQuestion'}}

<ul>
  {{#each quiz.questions as |question|}}
    {{create-question-form
      question=question
      createQuestion='createQuestion'
      deleteQuestion='deleteQuestion'
    }}
  {{/each}}
</ul>
