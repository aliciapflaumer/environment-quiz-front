<h3 class="quiz-header">{{quiz.title}}</h3>

<h1>question list: this is the place for quiz</h1>

{{question-list/question createQuestion='createQuestion'}}

<ul>
  {{#each quiz.questions as |question|}}
    {{question-list/question
      question=question
      createQuestion='createQuestion'
      deleteQuestion='deleteQuestion'
    }}
  {{/each}}
</ul>
